
public class Employee {

	int Employe_id;
	String Emp_Name;
	int Empsal;
	Employee(){
		System.out.println("hi iam Constuctor");
	}

	void detailsOfEmployee() {
		System.out.println(Employe_id + " " + Emp_Name + " " + Empsal);
	}

	public static void main(String[] args) {

		Employee e1 = new Employee();
		e1.Emp_Name = "kumar";
		e1.Empsal = 20000;
		e1.Employe_id = 001;
		e1.detailsOfEmployee();

		Employee e2 = new Employee();
		e2.Emp_Name = "murali";
		e2.Empsal = 40000;
		e2.Employe_id = 003;
		e2.detailsOfEmployee();

	}
}